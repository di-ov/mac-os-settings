// Place your key bindings in this file to override the defaultsauto[]
[
    {
      "key": "cmd+n",
      "command": "explorer.newFile"
    },
    {
      "key": "cmd+n",
      "command": "-workbench.action.files.newUntitledFile"
    },
    {
      "key": "shift+f4",
      "command": "openInTerminal"
    },
    {
      "key": "alt+left",
      "command": "workbench.action.navigateBack"
    },
    {
      "key": "cmd+alt+-",
      "command": "-workbench.action.navigateBack"
    },
    {
      "key": "cmd+alt+pageup",
      "command": "workbench.action.moveEditorToNextGroup"
    },
    {
      "key": "cmd+alt+right",
      "command": "-workbench.action.moveEditorToNextGroup"
    },
    {
      "key": "cmd+alt+pagedown",
      "command": "workbench.action.moveEditorToPreviousGroup"
    },
    {
      "key": "cmd+alt+left",
      "command": "-workbench.action.moveEditorToPreviousGroup"
    },
    {
      "key": "alt+tab",
      "command": "workbench.action.nextEditor"
    },
    {
      "key": "cmd+pagedown",
      "command": "-workbench.action.nextEditor"
    },
    {
      "key": "shift+alt+tab",
      "command": "workbench.action.previousEditor"
    },
    {
      "key": "cmd+pageup",
      "command": "-workbench.action.previousEditor"
    },
    {
      "key": "cmd+'",
      "command": "editor.action.revealDefinition",
      "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
      "key": "f12",
      "command": "-editor.action.revealDefinition",
      "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
      "key": "f12",
      "command": "editor.action.peekImplementation",
      "when": "editorHasImplementationProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
      "key": "cmd+shift+f12",
      "command": "-editor.action.peekImplementation",
      "when": "editorHasImplementationProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
      "key": "cmd+o",
      "command": "workbench.action.quickOpen"
    },
    {
      "key": "cmd+p",
      "command": "-workbench.action.quickOpen"
    },
    {
      "key": "cmd+p",
      "command": "workbench.action.showCommands"
    },
    {
      "key": "cmd+shift+p",
      "command": "-workbench.action.showCommands"
    },
    {
      "key": "cmd+]",
      "command": "editor.unfold",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+shift+]",
      "command": "-editor.unfold",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+[",
      "command": "editor.fold",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+shift+[",
      "command": "-editor.fold",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+shift+d",
      "command": "editor.action.selectHighlights",
      "when": "editorFocus"
    },
    {
      "key": "cmd+shift+l",
      "command": "-editor.action.selectHighlights",
      "when": "editorFocus"
    },
    {
      "key": "cmd+alt+right",
      "command": "cursorEnd",
      "when": "textInputFocus"
    },
    {
      "key": "end",
      "command": "-cursorEnd",
      "when": "textInputFocus"
    },
    {
      "key": "cmd+alt+left",
      "command": "cursorHome",
      "when": "textInputFocus"
    },
    {
      "key": "home",
      "command": "-cursorHome",
      "when": "textInputFocus"
    },
    {
      "key": "cmd+up",
      "command": "spaceBlockJumper.moveUp"
    },
    {
      "key": "cmd+down",
      "command": "spaceBlockJumper.moveDown"
    },
    {
      "key": "cmd+shift+up",
      "command": "spaceBlockJumper.selectUp"
    },
    {
      "key": "cmd+shift+down",
      "command": "spaceBlockJumper.selectDown"
    },
    {
      "key": "cmd+[",
      "command": "editor.foldAll",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+k cmd+0",
      "command": "-editor.foldAll",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+]",
      "command": "editor.unfoldAll",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+k cmd+j",
      "command": "-editor.unfoldAll",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "cmd+u",
      "command": "editor.action.deleteLines",
      "when": "textInputFocus && !editorReadonly"
    },
    {
      "key": "cmd+shift+k",
      "command": "-editor.action.deleteLines",
      "when": "textInputFocus && !editorReadonly"
    },
    {
      "key": "cmd+shift+alt+left",
      "command": "cursorHomeSelect",
      "when": "textInputFocus"
    },
    {
      "key": "shift+home",
      "command": "-cursorHomeSelect",
      "when": "textInputFocus"
    },
    {
      "key": "cmd+shift+alt+right",
      "command": "cursorEndSelect",
      "when": "textInputFocus"
    },
    {
      "key": "shift+end",
      "command": "-cursorEndSelect",
      "when": "textInputFocus"
    },
    {
      "key": "cmd+e",
      "command": "workbench.view.explorer"
    },
    {
      "key": "cmd+shift+e",
      "command": "-workbench.view.explorer"
    },
    {
      "key": "alt+right",
      "command": "workbench.action.navigateForward"
    },
    {
      "key": "cmd+shift+-",
      "command": "-workbench.action.navigateForward"
    },
    {
      "key": "cmd+alt+up",
      "command": "cursorPageUp",
      "when": "textInputFocus"
    },
    {
      "key": "pageup",
      "command": "-cursorPageUp",
      "when": "textInputFocus"
    },
    {
      "key": "cmd+alt+down",
      "command": "cursorPageDown",
      "when": "textInputFocus"
    },
    {
      "key": "pagedown",
      "command": "-cursorPageDown",
      "when": "textInputFocus"
    },
    {
      "key": "pageup",
      "command": "workbench.action.gotoLine"
    },
    {
      "key": "cmd+g",
      "command": "-workbench.action.gotoLine"
    },
    {
      "key": "pagedown",
      "command": "workbench.action.gotoSymbol"
    },
    {
      "key": "cmd+shift+o",
      "command": "-workbench.action.gotoSymbol"
    },
    {
      "key": "cmd+g",
      "command": "workbench.view.scm",
      "when": "gitlens:enabled && config.gitlens.keymap == 'chorded'"
    },
    {
      "key": "cmd+shift+g g",
      "command": "-workbench.view.scm",
      "when": "gitlens:enabled && config.gitlens.keymap == 'chorded'"
    },
    {
        "key": "cmd+right",
        "command": "cursorWordPartRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+right",
        "command": "-cursorWordPartRight",
        "when": "textInputFocus"
    },
    {
        "key": "shift+cmd+right",
        "command": "cursorWordPartRightSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+alt+right",
        "command": "-cursorWordPartRightSelect",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+left",
        "command": "cursorWordPartLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+left",
        "command": "-cursorWordPartLeft",
        "when": "textInputFocus"
    },
    {
        "key": "shift+cmd+left",
        "command": "cursorWordPartLeftSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+alt+left",
        "command": "-cursorWordPartLeftSelect",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+space",
        "command": "editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
    },
    {
        "key": "alt+escape",
        "command": "-editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
    }
]